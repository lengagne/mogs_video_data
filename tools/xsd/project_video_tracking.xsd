<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xs:element name="Joysticks">
<xs:complexType>
	<xs:sequence >
		<xs:element name="Joystick" minOccurs="0" maxOccurs="1"/>
	</xs:sequence>
</xs:complexType>
</xs:element>

<xs:element name="project">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="Definitions"/>
			<xs:element name="Datas"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="Definitions">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="videos"/>
			<xs:element name="points"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>



</xs:schema>


